{
  "version": 2,
  "builds": [
    {
      "src": "src/**/*",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/embed-reviews",
      "dest": "/embed-reviews.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
